<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <bean id="T1000" class="main.ModelT1000">
   		<constructor-arg ref = "HeadSony" />
   		<constructor-arg ref = "HandToshiba" />
   		<constructor-arg ref = "LeagToshiba" />
   </bean>
   
   <bean id="HandSony" class="sony.HandSony"/>
   <bean id="HeadSony" class="sony.HeadSony"/>
   <bean id="LeagSony" class="sony.LeagSony"/>
   
   <bean id="HandToshiba" class="toshiba.HandToshiba"/>
   <bean id="HeadToshiba" class="toshiba.HeadToshiba"/>
   <bean id="LeagToshiba" class="toshiba.LeagToshiba"/>

</beans>